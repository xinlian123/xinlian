public class Solution {
    public int[] countBits(int num) {
    int[] res = new int[num.length+1];
    for(int i =0; i<num.length;i++){
        res[i] = num[i];
    }return res;
    }
    public int countbit(int num){
        int bit=0;
        while(num%2==1){
            bit++;
            num=num/2;
        }return bit;
    }
}
